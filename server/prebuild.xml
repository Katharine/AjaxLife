<?xml version="1.0" ?>
<Prebuild version="1.10" xmlns="http://dnpb.sourceforge.net/schemas/prebuild-1.10.xsd">
  <Solution activeConfig="Debug" name="AjaxLife" path="./">
    <Configuration name="Debug">
      <Options>
        <CompilerDefines>TRACE;DEBUG</CompilerDefines>
        <OptimizeCode>false</OptimizeCode>
        <CheckUnderflowOverflow>false</CheckUnderflowOverflow>
        <AllowUnsafe>true</AllowUnsafe>
        <WarningLevel>4</WarningLevel>
        <WarningsAsErrors>false</WarningsAsErrors>
        <SuppressWarnings/>
        <OutputPath>bin</OutputPath>
        <DebugInformation>true</DebugInformation>
        <IncrementalBuild>true</IncrementalBuild>
        <NoStdLib>false</NoStdLib>
      </Options>
    </Configuration>
    <Configuration name="Release">
      <Options>
        <CompilerDefines>TRACE</CompilerDefines>
        <OptimizeCode>true</OptimizeCode>
        <CheckUnderflowOverflow>false</CheckUnderflowOverflow>
        <AllowUnsafe>true</AllowUnsafe>
        <WarningLevel>4</WarningLevel>
        <WarningsAsErrors>false</WarningsAsErrors>
        <SuppressWarnings/>
        <OutputPath>bin</OutputPath>
        <DebugInformation>false</DebugInformation>
        <IncrementalBuild>true</IncrementalBuild>
        <NoStdLib>false</NoStdLib>
      </Options>
    </Configuration>
    <Project frameworkVersion="v3_5" name="AjaxLife" path="./">
      <Configuration name="Debug">
        <Options>
          <OutputPath>bin/</OutputPath>
        </Options>
      </Configuration>
      <Configuration name="Release">
        <Options>
          <OutputPath>bin/</OutputPath>
        </Options>
      </Configuration>
      <ReferencePath>assemblies/</ReferencePath>
      <ReferencePath>assemblies/libopenmetaverse/bin/</ReferencePath>
      <Reference name="System" />
      <Reference name="System.Core"/>
      <Reference name="System.Data" />
      <Reference name="System.Drawing" />
      <Reference name="System.Web" />
      <Reference name="System.Xml" />
      <Reference name="Affirma.ThreeSharp"/>
      <Reference name="log4net"/>
      <Reference name="MiniHttpd"/>
      <Reference name="Newtonsoft.Json"/>
      <Reference name="OpenMetaverse"/>
      <Reference name="OpenMetaverse.StructuredData"/>
      <Reference name="OpenMetaverseTypes"/>
      <Reference name="XMLRPC"/>
      <Files>
        <Exclude pattern="bin"/>
        <Match pattern="*.cs" recurse="false"/>
        <Match pattern="Html/*.cs" recurse="true"/>
        <Match pattern="Html/Templates/*.cs" recurse="true"/>
        <Match pattern="Converters/*.cs" recurse="true"/>
        <Match pattern="libopenjpeg-dotnet-*.so" recurse="false" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)"/>
        <Match pattern="libopenjpeg-dotnet-*.dylib" recurse="false" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)"/>
        <Match pattern="OpenMetaverse.dll.config" recurse="false" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)"/>
        <Match pattern="assemblies/*.dll" recurse="false" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)"/>
        <Match pattern="*.txt" recurse="false" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)"/>
        <Match pattern="openmetaverse_data/*.tga" recurse="false" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/openmetaverse_data"/>
        <Match pattern="Html/Templates/*.html" recurse="false" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/Html/Templates"/>
        <Match pattern="assemblies/libopenmetaverse/bin/OpenMetaverse.dll" recurse="false" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)"/>
        <Match pattern="assemblies/libopenmetaverse/bin/OpenMetaverseTypes.dll" recurse="false" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)"/>
        <Match pattern="assemblies/libopenmetaverse/bin/OpenMetaverse.StructuredData.dll" recurse="false" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)"/>
        <Match pattern="assemblies/libopenmetaverse/bin/XMLRPC.dll" recurse="false" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)"/>

        <Match pattern="client/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client"/>

        <Match pattern="client/images/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/images"/>
        <Match pattern="client/images/inventory/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/images/inventory"/>
        <Match pattern="client/images/simdown/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/images/simdown"/>

        <Match pattern="client/resources/license.txt" recurse="false" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/license.txt"/>

        <Match pattern="client/resources/css/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/css"/>

        <Match pattern="client/resources/images/aero/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/aero"/>
        <Match pattern="client/resources/images/aero/basic-dialog/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/aero/basic-dialog"/>
        <Match pattern="client/resources/images/aero/grid/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/aero/grid"/>
        <Match pattern="client/resources/images/aero/layout/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/aero/layout"/>
        <Match pattern="client/resources/images/aero/qtip/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/aero/qtip"/>
        <Match pattern="client/resources/images/aero/sizer/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/aero/sizer"/>
        <Match pattern="client/resources/images/aero/tabs/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/aero/tabs"/>
        <Match pattern="client/resources/images/aero/toolbar/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/aero/toolbar"/>

        <Match pattern="client/resources/images/default/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default"/>
        <Match pattern="client/resources/images/default/basic-dialog/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default/basic-dialog"/>
        <Match pattern="client/resources/images/default/box/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default/box"/>
        <Match pattern="client/resources/images/default/dd/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default/dd"/>
        <Match pattern="client/resources/images/default/form/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default/form"/>
        <Match pattern="client/resources/images/default/grid/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default/grid"/>
        <Match pattern="client/resources/images/default/layout/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default/layout"/>
        <Match pattern="client/resources/images/default/menu/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default/menu"/>
        <Match pattern="client/resources/images/default/qtip/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default/qtip"/>
        <Match pattern="client/resources/images/default/shared/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default/shared"/>
        <Match pattern="client/resources/images/default/sizer/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default/sizer"/>
        <Match pattern="client/resources/images/default/tabs/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default/tabs"/>
        <Match pattern="client/resources/images/default/toolbar/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default/toolbar"/>
        <Match pattern="client/resources/images/default/tree/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/images/default/tree"/>

        <Match pattern="client/resources/script.aculo.us/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/resources/script.aculo.us"/>

        <Match pattern="client/sounds/*" recurse="true" buildAction="Copy" copyToOutput="PreserveNewest" destination="$(OutputPath)/client/sounds"/>
      </Files>
    </Project>
  </Solution>
</Prebuild>
